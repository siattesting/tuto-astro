    <meta charset="utf-8" />
    <link rel="icon" type="image/svg+xml" href="/favicon.ico" />
    <meta name="viewport" content="width=device-width" />
    <meta name="generator" content={Astro.generator} />
    <title>My Astro Site</title>
  </head>
  <body>
    <Navigation />
    <h1>My Astro Site</h1>
    <Footer />
  </body>
</html>
